{
    "name": "yii2/magazyn",
    "description": "Magazyn - projekt na prace inynierskÄ…",
    "keywords": ["yii2"],
    "homepage": "https://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kamil Czurak",
            "email": "k.cz@opoczta.pl",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqlnd": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "yiisoft/yii2": "~2.0.45",
        "yiister/yii2-gentelella": "~1.3",
        "rmrevin/yii2-fontawesome": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0.10",
        "yiisoft/yii2-swiftmailer": "~2.1.2",
        "yiisoft/yii2-queue": "^2.3.0",
        "yiisoft/yii2-redis": "~2.0.13",
        "yiisoft/http": "^1.0.0",
        "yiisoft/strings": "^2.0.0",
        "yiisoft/files": "^1.0.0",
        "kartik-v/yii2-widget-datetimepicker": "^1.4.9",
        "kartik-v/yii2-widget-select2": "^2.2.0",
        "kartik-v/yii2-widget-switchinput": "^1.3.0",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "spatie/image-optimizer": "^1.2.0",
        "yiisoft/yii2-imagine": "^2.2.0",
        "oscarotero/inline-svg": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~9.5.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/lib-innerbrowser": "^4.0 || ^3.0 || ^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^3.0 || ^2.0 || ^1.1",
        "codeception/verify": "^3.0 || ^2.2",
        "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4",
        "bizley/migration": "^4.1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "common\\tests\\": ["common/tests/", "common/tests/_support"],
            "backend\\tests\\": ["backend/tests/", "backend/tests/_support"],
            "frontend\\tests\\": ["frontend/tests/", "frontend/tests/_support"]
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
